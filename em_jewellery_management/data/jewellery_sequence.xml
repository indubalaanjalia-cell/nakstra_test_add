<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="seq_jewellery_job_work" model="ir.sequence">
        <field name="name">Jewellery Job Work</field>
        <field name="code">jewellery.job.work</field>
        <field name="prefix">JW/</field>
        <field name="padding">5</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_jewellery_issue_voucher" model="ir.sequence">
        <field name="name">Jewellery Issue Voucher</field>
        <field name="code">jewellery.issue.voucher</field>
        <field name="prefix">IV/</field>
        <field name="padding">5</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_jewellery_receive_voucher" model="ir.sequence">
        <field name="name">Jewellery Receive Voucher</field>
        <field name="code">jewellery.receive.voucher</field>
        <field name="prefix">RV/</field>
        <field name="padding">5</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_refinery" model="ir.sequence">
        <field name="name">Refinery Order</field>
        <field name="code">jewellery.refinery</field>
        <field name="prefix">REF/</field>
        <field name="padding">5</field>
    </record>

</odoo>
